{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "background": {
    "scripts": ["./src/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./src/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./src/scripts/script.js"]
    }
  ],
  "manifest_version": 2
}
